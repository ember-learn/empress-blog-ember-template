{
  "name": "empress-blog-ember-template",
  "version": "3.0.0",
  "description": "Ember template for empress-blog",
  "keywords": [
    "ember-addon",
    "empress-blog",
    "empress-blog-template"
  ],
  "repository": "https://github.com/ember-learn/empress-blog-ember-template",
  "license": "MIT",
  "author": {
    "name": "Chris Manson",
    "email": "chris@stonecircle.io",
    "url": "https://stonecircle.io"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "auto-changelog --template changelog.template --unreleased-only --prepend --load-github-issue-data --github-cache-dir .changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "broccoli-concat": "^4.2.4",
    "broccoli-funnel": "^3.0.3",
    "ember-auto-import": "^2.6.2",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^5.7.2",
    "ember-composable-helpers": "^3.2.0",
    "ember-concurrency": "^2.2.0",
    "ember-showdown-prism": "^3.0.0",
    "ember-styleguide": "^8.0.0",
    "ember-svg-jar": "^2.3.3",
    "ember-test-waiters": "^2.1.3"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@percy/cli": "^1.21.0",
    "@percy/ember": "^3.0.1",
    "auto-changelog": "github:mansona/auto-changelog#epic",
    "babel-eslint": "^10.1.0",
    "babel-plugin-ember-template-compilation": "^2.0.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.28.5",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^4.1.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~3.28.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-test-selectors": "^6.0.0",
    "ember-try": "^4.0.0",
    "empress-blog": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "lint-to-the-future": "^0.8.0",
    "lint-to-the-future-ember-template": "^0.3.5",
    "lint-to-the-future-eslint": "^0.3.1",
    "loader.js": "^4.7.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prember": "^1.1.1",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^1.6.0",
    "webpack": "^5.65.0"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd",
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
